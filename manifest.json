{
    "manifest_version": 3,
    "name": "Tab Finder",
    "version": "1.0",
    "description": "A Chrome extension to search and switch between open tabs.",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "incognito": "split"
  }